{
  "kind": "collectionType",
  "collectionName": "practice_areas",
  "info": {
    "singularName": "practice-area",
    "pluralName": "practice-areas",
    "displayName": "PracticeAreas"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "thumbnailImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "lists": {
      "type": "component",
      "component": "lists.lists",
      "repeatable": true
    },
    "lawyers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lawyer.lawyer",
      "mappedBy": "lawyer"
    }
  }
}
